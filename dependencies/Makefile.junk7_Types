.obj$(CFG)/junk_7SplDcps$(OBJEXT): data_type_files/junk_7SplDcps.cpp
	@$(TESTDIRSTART) ".obj$(CFG)/" $(TESTDIREND) $(MKDIR) ".obj$(CFG)/"
	$(COMPILE.cc) $(EXPORTFLAGS) $(OUTPUT_OPTION) data_type_files/junk_7SplDcps.cpp

.obj$(CFG)/junk_7$(OBJEXT): data_type_files/junk_7.cpp
	@$(TESTDIRSTART) ".obj$(CFG)/" $(TESTDIREND) $(MKDIR) ".obj$(CFG)/"
	$(COMPILE.cc) $(EXPORTFLAGS) $(OUTPUT_OPTION) data_type_files/junk_7.cpp

clean:
