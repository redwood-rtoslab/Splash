# -*-Makefile-*-
#----------------------------------------------------------------------------
#       Macros
#----------------------------------------------------------------------------

CC            = gcc
CXX           = g++
LD            = $(CXX) $(CCFLAGS) $(PICFLAGS) $(GENFLAGS)
AR            = ar
PICFLAGS      = -fPIC
CCFLAGS        =  
CPPFLAGS      = $(PICFLAGS) $(GENFLAGS) -D_REENTRANT -D_POSIX_C_SOURCE=200112L -D_XOPEN_SOURCE=500 -Wall -Wno-unused-function -lstdc++ -std=c++0x -I"/home/bjyang/HDE/x86_64.linux/include" -I"/home/bjyang/HDE/x86_64.linux/include/sys" -I"$(BOOST_ROOT)/." -I"/home/bjyang/HDE/x86_64.linux/include/dcps/C++/isocpp2" -I"/home/bjyang/HDE/x86_64.linux/include/dcps/C++/SACPP" -I"." 
OBJEXT        = .o
OUTPUT_OPTION = -o "$@"
COMPILE.c     = $(CC) $(CFLAGS) $(CPPFLAGS) -c
COMPILE.cc    = $(CXX) $(CCFLAGS) $(CPPFLAGS) -c
LDFLAGS       = -L"." -L"/home/bjyang/HDE/x86_64.linux/lib" -Wl,-rpath,\$$ORIGIN -Wl,-rpath,\$$/home/bjyang/HDE/x86_64.linux/lib
CCC           = $(CXX)
MAKEFILE      = Makefile.Cruise_proxy_gear
DEPENDENCIES  = $(MAKEFILE)
BTARGETDIR    = ./
BIN	      = $(BTARGETDIR)Cruise_proxy_gear$(EXESUFFIX)$(EXEEXT)
CAT           = cat
MV            = mv -f
RM            = rm -rf
CP            = cp -p
NUL           = /dev/null
MKDIR         = mkdir -p
TESTDIRSTART  = test -d
TESTDIREND    = ||
TOUCH         = touch
EXEEXT        = 
LIBPREFIX     = lib
LIBSUFFIX     = 
GENFLAGS      = -O3 -DNDEBUG
LDLIBS        = -l"junk_9_Types$(LIBSUFFIX)" -l"junk_8_Types$(LIBSUFFIX)" -l"junk_7_Types$(LIBSUFFIX)" -l"junk_6_Types$(LIBSUFFIX)" -l"junk_5_Types$(LIBSUFFIX)" -l"junk_4_Types$(LIBSUFFIX)" -l"junk_3_Types$(LIBSUFFIX)" -l"junk_2_Types$(LIBSUFFIX)" -l"junk_1_Types$(LIBSUFFIX)" -l"Processing_block$(LIBSUFFIX)" -l"path_information_Types$(LIBSUFFIX)" -l"cruise_gear_Types$(LIBSUFFIX)" -l"cruise_speed_Types"$(LIBSUFFIX) -l"cruise_information_Types$(LIBSUFFIX)" -l"dcpsisocpp2$(LIBSUFFIX)" -l"ddskernel" -ldl $(subst lib,-l,$(sort $(basename $(notdir $(wildcard /usr/lib/librt.so /lib/librt.so))))) -lpthread
OBJS          = .obj$(CFG)/Cruise_proxy_gear$(OBJEXT)
SRC           = ../src/Cruise_proxy_gear.cpp
LINK.cc       = $(LD) $(LDFLAGS)
EXPORTFLAGS   = 

#----------------------------------------------------------------------------
#       Local targets
#----------------------------------------------------------------------------

all: $(BIN)

$(BIN): $(OBJS)
	@$(TESTDIRSTART) "$(BTARGETDIR)" $(TESTDIREND) $(MKDIR) "$(BTARGETDIR)"
	$(LINK.cc) $(OBJS) $(LDLIBS) $(OUTPUT_OPTION)

generated: $(GENERATED_DIRTY)
	@-:

.obj$(CFG)/Cruise_proxy_gear$(OBJEXT): ../src/Cruise_proxy_gear.cpp
	@$(TESTDIRSTART) ".obj$(CFG)/" $(TESTDIREND) $(MKDIR) ".obj$(CFG)/"
	$(COMPILE.cc) $(EXPORTFLAGS) $(OUTPUT_OPTION) ../src/Cruise_proxy_gear.cpp

clean:
	-$(RM) $(OBJS)

realclean: clean
	-$(RM) $(SHLIB) $(LIB)
	-$(RM) .obj$(CFG)/

check-syntax:
	$(COMPILE.cc) $(EXPORTFLAGS) $(CHK_SOURCES)

